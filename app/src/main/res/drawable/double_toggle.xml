<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <shape android:shape="rectangle" android:tint="?attr/colorAlphaAccent">
            <corners android:radius="@dimen/ted" />
            <solid android:color="#FFFFFFFF" />
            <size android:height="@dimen/ted" />
        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle" android:tint="?attr/colorPrimaryAlpha">
            <corners android:radius="@dimen/ted" />
            <solid android:color="#FFFFFFFF" />
            <size android:height="@dimen/ted" />
        </shape>
    </item>
</selector>