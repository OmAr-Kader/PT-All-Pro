<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape>
            <solid android:color="#00000000" />
            <stroke
                android:width="@dimen/bcd"
                android:color="#FFFFFF" />
            <corners android:radius="@dimen/card_radius_normal" />
        </shape>
    </item>
    <item>
        <ripple android:color="?attr/rmoText">
            <item android:id="@android:id/mask">
                <shape android:shape="rectangle">
                    <solid android:color="@color/psc" />
                    <corners android:radius="@dimen/card_radius_normal" />
                </shape>
            </item>
        </ripple>
    </item>
</layer-list>